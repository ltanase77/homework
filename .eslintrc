{
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "es6": true
  },
  "rules": {
    "indent": [2, 4],
    "quotes": "off",
    "linebreak-style": ["error", "windows"],
    "max-len": ["error", {"code": 120}],
    "require-jsdoc": 0,
    "comma-dangle": "warn",
    "no-unused-vars": ["error", {"vars": "local", "args": "none"}]
  }
}